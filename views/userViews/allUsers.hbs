<main>
    <div class="container">
        <div class="row">
            <a href="/userViews/rates">
                <btn class="btn btn-primary">Manage Rates</btn>
            </a>
        </div>
        <div class="row">
            {{#each users}}
                <div class="col-lg-3">
                    <div class="profile-well">
                        <img class="img img-circle center-block" src="/images/lora.jpg" width="150" height="150">

                        <h5 class="text-center">{{this.fullName}}</h5>

                        <div class="">
                            <p>E-mail: {{this.email}}</p>
                            <p>Phone: {{this.phone}}</p>
                        </div>

                        <a href="/userViews/userProfile/{{this.id}}" class="btn btn-default center-block">View Profile</a>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
</main>